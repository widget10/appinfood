<%- include('./includes/header.ejs') %>

<div class="jumbotron text-center">
	<h4>MENU</h4>

</div>
<div class=" container my-5 mx-3">

	
			


	<div class="row">
			<% if(food.length>0) { %>
				<% for(var i=0; i<food.length;i++) { %>

		<div class="col-sm-6 col-md-4">
			<div class="thumbnail">
				<img src="https://upload.wikimedia.org/wikipedia/en/5/5e/Gothiccover.png" alt="..." class="img-responsive">
				<div class="caption">
					<h3><%= food[i].foodname %></h3>
					<p class="description"><%= food[i].des %></p>
					<div class="clearfix">
						<div class="price pull-left"><%= food[i].price %></div>
						<a href="customer/add-to-cart/<%- food[i]._id %>" class="btn btn-success pull-right" role="button">ADD to cart</a>
					</div>
				</div>
			</div>
		</div>
		<% } %>
		<% } %>
		
		

</div>
	
</div>
